#include <iostream>
#include <queue>
#include <vector>
using namespace std;

void bfsMatrix(int adj[10][10], int n, int start) {
    bool visited[10] = {false};
    queue<int> q;
    visited[start] = true;
    q.push(start);
    cout << "BFS Traversal (Adjacency Matrix): ";
    while (!q.empty()) {
        int node = q.front();
        q.pop();
        cout << node << " ";
        for (int i = 0; i < n; i++) {
            if (adj[node][i] == 1 && !visited[i]) {
                visited[i] = true;
                q.push(i);
            }
        }
    }
    cout << endl;
}

void bfsList(vector<int> adjList[], int n, int start) {
    bool visited[10] = {false};
    queue<int> q;
    visited[start] = true;
    q.push(start);
    cout << "BFS Traversal (Adjacency List): ";
    while (!q.empty()) {
        int node = q.front();
        q.pop();
        cout << node << " ";
        for (int neighbor : adjList[node]) {
            if (!visited[neighbor]) {
                visited[neighbor] = true;
                q.push(neighbor);
            }
        }
    }
    cout << endl;
}

int main() {
    int n, edges;
    cout << "Enter number of vertices: ";
    cin >> n;
    cout << "Enter number of edges: ";
    cin >> edges;
    int adjMatrix[10][10] = {0};
    vector<int> adjList[10];
    cout << "Enter edges (u v): " << endl;
    for (int i = 0; i < edges; i++) {
        int u, v;
        cin >> u >> v;
        adjMatrix[u][v] = 1;
        adjMatrix[v][u] = 1;
        adjList[u].push_back(v);
        adjList[v].push_back(u);
    }
    int start;
    cout << "Enter starting vertex for BFS: ";
    cin >> start;
    bfsMatrix(adjMatrix, n, start);
    bfsList(adjList, n, start);
    return 0;
}
